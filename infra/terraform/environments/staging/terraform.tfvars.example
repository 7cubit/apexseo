# API Keys
serper_api_key = "CHANGE_ME"

# NextAuth
nextauth_secret = "CHANGE_ME_RANDOM_STRING" # Generate with: openssl rand -base64 32

# Google OAuth (optional, for GSC integration)
google_client_id     = ""
google_client_secret = ""
